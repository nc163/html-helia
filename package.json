{
  "name": "html-ipfs",
  "description": "html-ipfs is a library for fetch data from IPFS and displaying it on a web page.",
  "version": "0.0.7",
  "homepage": "https://github.com/nc163/html-ipfs",
  "bugs": {
    "url": "https://github.com/nc163/html-ipfs/issues"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "http://github.com/nc163/html-ipfs.git"
  },
  "types": "dist/",
  "type": "module",
  "scripts": {
    "build": "npm run build:core && npm run build:html-ipfs-element",
    "build:core": "npm run build --prefix packages/core",
    "build:html-ipfs-element": "npm run build --prefix packages/html-ipfs-element"
  },
  "keywords": [
    "browser",
    "ipfs",
    "dapps"
  ],
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "@types/node": "^18.16.3",
    "browserify": "^17.0.0",
    "esmify": "^2.1.1",
    "jest": "^29.6.1",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.2",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4"
  },
  "workspaces": [
    "packages/core",
    "packages/html-ipfs-element",
    "packages/types"
  ]
}
