{
  "name": "ipfs-tag",
  "description": "ipfs-tag is a library for fetch data from IPFS and displaying it on a web page.",
  "version": "0.0.7",
  "homepage": "https://github.com/nc163/ipfs-tag",
  "bugs": {
    "url": "https://github.com/nc163/ipfs-tag/issues"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "http://github.com/nc163/ipfs-tag.git"
  },
  "type": "module",
  "scripts": {
    "build": "npm run build:core && npm run build:type && npm run build:html && npm run build:react",
    "build:core": "npm run build --prefix packages/core",
    "build:type": "npm run build --prefix packages/type",
    "build:html": "npm run build --prefix packages/html",
