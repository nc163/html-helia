{
  "compilerOptions": {
    "declaration": true,
    "outDir": "./dist",
    "paths": {
      "@html-ipfs/core": ["packages/core"],
      "@html-ipfs/html-ipfs-tag": ["packages/html-ipfs-tag"],
      "@html-ipfs/html-ipfs-element": ["packages/html-ipfs-element"],
      "*": ["node_modules/*"],
    },
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "exclude": [
    "node_modules",
    "dist"
  ],
  "plugins": [
  ]
}
