  },
  "keywords": [
    "browser",
    "ipfs",
    "dapps"
  ],
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^18.16.3",
    "express": "^4.18.2",
    "rimraf": "^5.0.0",
    "ts-loader": "^9.4.2",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4",
    "webpack": "^5.83.1",
    "webpack-cli": "^5.1.1"
  },
  "workspaces": [
    "packages/core",
    "packages/react",
    "packages/html",
    "packages/types"
  ]
}
